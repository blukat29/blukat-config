# ~/alises: Part of .bashrc for alises.

alias ll='ls -al'
alias ls='ls -F --color=auto'
alias dir='ls'
alias cls='clear'
alias grepc='grep -2 -n --color=auto'
alias grep='grep --color=auto'
alias tma='tmux attach -t'
alias tl='tmux ls'
alias tmux='tmux -2'
alias ww='w | grep `whoami`'
alias less='less -R'
alias g='git'

